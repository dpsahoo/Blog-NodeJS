<%- include("partials/header"); -%> 


<h1>Home</h1>
<p><%= startingContent %></p>

<!-- Render each blog post on the HomePage -->
<% posts.forEach(function(post) { %>
   <h1> <%= post.title%> </h1>
   <p> 
    <%= post.content.substring(0, 100) + " ..." %> 
    <a href="/posts/<%= post.title %>">Read More</a>
    </p>

<% }); %>

<%- include("partials/footer"); -%> 